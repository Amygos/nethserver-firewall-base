#
# 90ndpi
# Restore all markers
#
{
   use NethServer::Firewall;

   my $fw = new NethServer::Firewall();
   return '' if (!$fw->isNdpiEnabled());
   return '' if (! %protocols);

   $OUT .= "# Save the current packet mark in the conntrack table if it's already marked\n";
   $OUT .= "SAVE:F          -               -               -     -       -       -       !0x000/0xffff";
}
