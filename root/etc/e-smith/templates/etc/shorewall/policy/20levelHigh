{
    my $nfqueue = $firewall{'nfqueue'} || 'disabled';
    if ($nfqueue eq 'enabled') {
        $OUT.="loc		net		NFQUEUE";
    } else {
        $OUT.="loc		net		ACCEPT";
    }
}
loc		$FW		REJECT		info
loc		all		REJECT		info

$FW		net		ACCEPT		
$FW		loc		ACCEPT

net		$FW		DROP		info
net		loc		DROP		info
net		all		DROP		info



